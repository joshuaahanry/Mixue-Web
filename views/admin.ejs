  <style>
    body {
      margin: 0;
      font-family: Arial, sans-serif;
      background-color: #f4f4f4;
      display: flex;
    }

.sidebar {   
  top: 0;
  left: 0;
  width: 300px;
  height: 1500vh;       
  background-color: #E4181F;
  color: white;
  padding: 20px;
  display: flex;
  flex-direction: column;
  transition: transform 0.3s ease;
  z-index: 1000;          
}


    .logo {
      display: flex;
      align-items: center;
      gap: 10px;
      margin-bottom: 30px;
    }

    .logo img {
      height: 50px;
    }

    .nav-item {
      background-color: rgba(255,255,255,0.15);
      border-radius: 8px;
      padding: 12px;
      margin-bottom: 12px;
      display: flex;
      align-items: center;
      cursor: pointer;
      justify-content: space-between;
    }

    .nav-item.primary {
      background-color: white;
      color: #E4181F;
    }

    .icon {
      margin-right: 10px;
    }

    .badgenew {
      background: white;
      color: #E4181F;
      font-size: 12px;
      border-radius: 12px;
      padding: 2px 8px;
    }

    @media (max-width: 768px) {
      .sidebar {
        position: fixed;
        top: 0;
        left: 0;
        bottom: 0;
        transform: translateX(-100%);
        z-index: 1000;
      }

      .sidebar.open {
        transform: translateX(0);
      }

      .menu-toggle {
        position: fixed;
        top: 20px;
        left: 20px;
        background-color: #E4181F;
        color: white;
        border: none;
        border-radius: 4px;
        padding: 8px 12px;
        z-index: 1100;
        font-size: 18px;
      }
    }
  </style>
</head>
